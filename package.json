{
  "name": "contabilease",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:strict": "jest --ci --coverage --watchAll=false --maxWorkers=1",
    "test:update": "jest --updateSnapshot",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "quality-check": "npm run type-check && npm run lint:strict && npm run test:strict",
    "quality-check:quick": "npm run type-check && npm run lint:check && npm run format:check",
    "quality-check:fix": "npm run lint:fix && npm run format",
    "pre-commit": "npm run quality-check",
    "pre-commit:quick": "npm run quality-check:quick",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "security-check": "npm audit --audit-level high",
    "bundlesize": "npx bundlesize",
    "audit-ci": "npx audit-ci --moderate",
    "vercel-check": "node scripts/vercel-pre-deploy-check.js",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.39.0",
    "@testing-library/dom": "^10.4.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "^14.2.32",
    "next-intl": "^3.15.0",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "speakeasy": "^2.0.0",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^3.5.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "audit-ci": "^7.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5"
  }
}
